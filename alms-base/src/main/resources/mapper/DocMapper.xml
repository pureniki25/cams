<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hongte.alms.base.mapper.DocMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hongte.alms.base.entity.Doc">
        <id column="doc_id" property="docId" />
        <result column="business_id" property="businessId" />
        <result column="doc_name" property="docName" />
        <result column="doc_url" property="docUrl" />
        <result column="original_name" property="originalName" />
        <result column="file_size" property="fileSize" />
        <result column="file_type" property="fileType" />
        <result column="doc_type_id" property="docTypeId" />
        <result column="create_user" property="createUser" />
        <result column="create_time" property="createTime" />
        <result column="update_user" property="updateUser" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        doc_id AS docId, business_id AS businessId, doc_name AS docName, doc_url AS docUrl, original_name AS originalName, file_size AS fileSize, file_type AS fileType, doc_type_id AS docTypeId, create_user AS createUser, create_time AS createTime, update_user AS updateUser, update_time AS updateTime
    </sql>

</mapper>
