    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        type_id AS typeId, type_code AS typeCode, type_name AS typeName, can_item_running_count AS canItemRunningCount, can_item_total_count AS canItemTotalCount, create_user AS createUser, create_time AS createTime, update_user AS updateUser, update_time AS updateTime
