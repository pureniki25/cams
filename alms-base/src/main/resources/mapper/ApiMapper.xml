<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hongte.alms.base.mapper.ApiMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hongte.alms.base.entity.Api">
        <id column="api_id" property="apiId" />
        <result column="apikey" property="apikey" />
        <result column="apiname" property="apiname" />
        <result column="token" property="token" />
        <result column="developname" property="developname" />
        <result column="api_type" property="apiType" />
        <result column="status" property="status" />
        <result column="create_user" property="createUser" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        api_id AS apiId, apikey, apiname, token, developname, api_type AS apiType, status, create_user AS createUser, create_time AS createTime
    </sql>

</mapper>
